{
  "name": "website",
  "version": "1.0.0",
  "main": "build/server/index.js",
  "license": "MIT",
  "scripts": {
    "build:pages": "webpack --config build-config/client.webpack.js",
    "build:server": "webpack --config build-config/server.webpack.js",
    "copy:assets": "cp src/web/styles/global.css build/static/index.css && cp -R assets/* build/static/",
    "dev:server": "npm run build:pages && npm run copy:assets && ts-node src/index.ts",
    "dev:client": "webpack serve --config build-config/development.webpack.js",
    "build": "yarn build:pages && yarn build:server && yarn copy:assets",
    "fmt": "prettier --write src/",
    "gh": "git log --graph --oneline",
    "start": "node build/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@nickgdev/porygon-metlib": "^1.2.2",
    "axios": "^1.3.4",
    "chalk": "4.1.2",
    "color-scales": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "6",
    "react-modal": "^3.16.1",
    "react-query": "^3.39.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@nickgdev/larvitar-types": "^1.0.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "json-loader": "^0.5.7",
    "prettier": "^2.8.3",
    "react-router-dom": "5",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
