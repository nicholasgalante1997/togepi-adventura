{
  "name": "website",
  "version": "1.0.0",
  "main": "build/index.js",
  "license": "MIT",
  "scripts": {
    "build:pages": "webpack --config build-config/client.webpack.js",
    "build:server": "webpack --config build-config/server.webpack.js",
    "copy:global-css-to-index": "cp src/web/styles/global.css build/static/index.css",
    "copy:global-css": "cp -R src/web/styles/global.css build/static/",
    "copy:vars-css": "cp -R src/web/styles/vars.css build/static/",
    "copy:make-pages-css-dir": "mkdir -p build/static/pages/",
    "copy:make-component-css-dir": "mkdir -p build/static/components/",
    "copy:components-css": "cp -R src/web/styles/components/*.css build/static/components/",
    "copy:pages-css": "cp -R src/web/styles/pages/*.css build/static/pages/",
    "copy:assets": "pnpm copy:global-css-to-index && pnpm copy:global-css && pnpm copy:vars-css && pnpm copy:make-pages-css-dir && pnpm copy:make-component-css-dir && pnpm copy:components-css && pnpm copy:pages-css && cp -R assets/* build/static/",
    "dev:server": "npm run build:pages && npm run copy:assets && ts-node src/index.ts",
    "dev:client": "webpack serve --config build-config/development.webpack.js",
    "build": "pnpm build:pages && pnpm build:server && pnpm copy:assets",
    "clean": "pnpm clean:build",
    "clean:build": "rm -rf build/",
    "fmt": "prettier --write src/",
    "lint": "eslint --fix src/",
    "git-history": "git log --graph --oneline",
    "sisyphus": "pnpm clean:build && pnpm build && pnpm start",
    "start": "node build/index.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "env:scripts": "node scripts/read-scripts.mjs",
    "gql-codegen:compile": "graphql-codegen",
    "gql-codegen:watch": "graphql-codegen -w",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@nickgdev/porygon-metlib": "^1.2.2",
    "axios": "^1.3.4",
    "chalk": "4.1.2",
    "color-scales": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "6",
    "react-modal": "^3.16.1",
    "react-query": "^3.39.3",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@nickgdev/larvitar-types": "^1.1.0",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/addon-mdx-gfm": "^7.0.5",
    "@storybook/react": "^7.0.5",
    "@storybook/react-webpack5": "^7.0.5",
    "@storybook/testing-library": "^0.1.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "bundle-stats-webpack-plugin": "^4.3.4",
    "css-loader": "^6.7.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "json-loader": "^0.5.7",
    "prettier": "^2.8.3",
    "react-router-dom": "5",
    "storybook": "^7.0.5",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "*",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "engines": {
    "node": ">=18.14.2"
  }
}
